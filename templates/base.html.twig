<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}Welcome!
			{% endblock %}
		</title>
		<link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text><text y=%221.3em%22 x=%220.2em%22 font-size=%2276%22 fill=%22%23fff%22>sf</text></svg>"> {% block stylesheets %}{% endblock %}

		{% block javascripts %}
			{% block importmap %}
				{{ importmap('app') }}
			{% endblock %}
		{% endblock %}
	</head>
	<body class="bg-slate-900 text-white">
		<header class="flex items-center justify-between p-5  border-b border-slate-400  font-bold">
			<div>
				<a href="{{path('app_homepage')}}">
					<img src="{{ asset('images/logo.png') }}" class="w-40" alt="Starshop Logo">
				</a>
			</div>
			<nav>
				<ul class="space-x-5">
					<a href="#">Home</a>
					<a href="#">About</a>
					<a href="#">Contact</a>
					<a href="#" class="bg-slate-600 p-2 rounded-lg">Get Started</a>
				</ul>
			</nav>
		</header>
		{% block body %}{% endblock %}
	</body>
</html>
